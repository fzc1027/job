/* 公司下拉 */
.sortup{
  width:100%;
}
.sort{
  border:1rpx #a9a9a9 solid;
overflow: hidden;
width: 100%;
margin-right: 50rpx;
position: absolute;
left: 0;
top: 160rpx;
background-color: white;

}
.sort .up{
animation-name:slidown; /*名称*/
animation-duration:0.3s; /*持续时间*/
animation-timing-function:linear; /*播放动画时从头到尾都以相同的速度*/
}
.sort .down{
animation-name:slidup; /*名称*/
animation-duration:0.3s; /*持续时间*/
animation-timing-function:linear; /*播放动画时从头到尾都以相同的速度*/
}
.sort .sort view{
border:1rpx #a9a9a9 solid;
height: 160rpx;
line-height: 80rpx;
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(51,51,51,1);
}
.sort .dropdown_active{
color:rgba(20,130,240,1);
}

/* css3下拉特效 */
@keyframes slidown{
from{
transform: translateY(-100%);
}
to{
transform: translateY(0%);
}
}

@keyframes slidup{
from{
transform: translateY(0%);
}
to{
transform: translateY(-100%);
}
}

