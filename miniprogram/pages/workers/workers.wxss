/*搜索框*/
.search-container {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding: 20rpx 20rpx;
    width: 100%;
    height:100rpx;
    background-color: #477CFE;
}




.search-container .search-input {
   width: 90%;
   height: 70rpx;
    background-color:#fff;
    border-radius: 5rpx;
    font-size: 24rpx;
    padding: 2rpx 30rpx;
    padding-left: 60rpx;
    color: #000;
    position: relative;
    margin: 0 20rpx;
    display: flex;
    align-items: center;
}

.search-container .search-input i{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    left: 20rpx;
    
    top: 50%;
    transform: translateY(-50%);
}

.search-container .search-input i image {
    width: 100%;
    height: 100%;
    object-fit: contain;
}
.place-holder {
    margin-top:8rpx;
    color: #e0e0e0;
    font-size: 30rpx;
}



